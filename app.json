{
	"expo": {
		"name": "drinker",
		"slug": "drinker",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.jpg",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#FAFAFC"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.drinker.app"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "5bceb9f7-5522-453c-8523-b6f1e079ec55"
			}
		}
	}
}
